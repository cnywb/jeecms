<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.jeecms.cms.entity.main">
	<class name="FordRepairWeb" table="FORD_REPAIR_WEB">
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
		<id name="vroId" type="java.lang.String" column="VRO_ID" length="30"/>
		<property name="vcardNo" column="VCARD_NO" type="string" not-null="false" length="60"/>
		<property name="vbillId" column="VBILL_ID" type="string" not-null="false" length="30"/>
		<property name="vsstId" column="VSST_ID" type="string" not-null="false" length="60"/>
		<property name="vsstName" column="VSST_NAME" type="string" not-null="false" length="200"/>
		<property name="vvin" column="VVIN" type="string" not-null="false" length="60"/>
		<property name="vrepairDate" column="VREPAIR_DATE" type="string" not-null="false" length="20"/>
		<property name="vfinishDate" column="VFINISH_DATE" type="string" not-null="false" length="20"/>
		<property name="vbillDate" column="VBILL_DATE" type="string" not-null="false" length="20"/>
		<property name="vrepairType" column="VREPAIR_TYPE" type="string" not-null="false" length="40"/>
		<property name="nmileage" column="NMILEAGE" type="long" not-null="false" length="10"/>
		<property name="nbalanceFee" column="NBALANCE_FEE" type="long" not-null="false" length="22"/>
		<property name="vcreated" column="VCREATED" type="string" not-null="false" length="100"/>
		<property name="dcrtDate" column="DCRT_DATE" type="timestamp" not-null="false" length="19"/>
	</class>
</hibernate-mapping>